<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.cuihai.ratingbarview.MainActivity">

    <com.cuihai.library.RatingBarView
        app:starCount="5"
        app:starFill="@mipmap/ic_launcher"
        app:starImageSize="20dp"
        android:id="@+id/star"
        app:starEmpty="@mipmap/ic_launcher"
        app:starPadding="20dp"
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="8dp" />
    <com.cuihai.library.RatingBarView
        app:starCount="5"
        android:layout_marginTop="20dp"
        app:starFill="@mipmap/ic_launcher_round"
        app:starImageSize="20dp"
        app:starEmpty="@mipmap/ic_launcher_round"
        app:starPadding="20dp"
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="8dp" />
    <com.cuihai.library.RatingBarView
        app:starCount="5"
        android:layout_marginTop="20dp"
        app:starFill="@drawable/star"
        app:starImageSize="20dp"
        app:starEmpty="@drawable/star"
        app:starPadding="20dp"
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="8dp" />

</LinearLayout>
